{
  "name": "dynamic-wallpaper",
  "version": "1.0.0",
  "description": "动态壁纸软件 - 支持视频壁纸播放",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "concurrently \"npm run dev:renderer\" \"wait-on http://localhost:3000 && electron .\"",
    "dev:renderer": "vite",
    "build": "vite build",
    "build:electron": "electron-builder",
    "dist": "npm run build && npm run build:electron"
  },
  "keywords": ["wallpaper", "dynamic", "electron", "video"],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "electron": "37.2.1",
    "electron-builder": "^24.9.1",
    "concurrently": "^8.2.2",
    "wait-on": "^7.2.0",
    "vite": "^5.0.8",
    "@vitejs/plugin-react": "^4.2.1"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "axios": "^1.6.2",
    "electron-store": "^8.1.0",
    "antd": "^5.12.8",
    "@ant-design/icons": "^5.2.6"
  },
  "build": {
    "appId": "com.dynamicwallpaper.app",
    "productName": "Dynamic Wallpaper",
    "directories": {
      "output": "dist"
    },
    "files": [
      "dist/**/*",
      "main.js",
      "preload.js"
    ],
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
